# Cryptic Vault SSH Honeypot Environment Variables
# COPY THIS FILE TO .env AND UPDATE WITH YOUR VALUES

# Required
GEMINI_API_KEY=your_api_key_here

# Flask Application (Optional but recommended for production)
FLASK_SECRET_KEY=generate_a_random_secret_key_here

# Optional (defaults shown)
SSH_HOST=0.0.0.0
SSH_PORT=2222
LOG_LEVEL=INFO

# Maximum concurrent SSH connections per IP
MAX_CONNECTIONS_PER_IP=3